{% extends 'master.jinja2' %}

{% block main %}

<article class="{{ index.name }}">
{{ index.content_html|safe }}
</article>

{% if index.leaves %}
    <div class="section-container">
        {% for leaf in index.leaves %}
            {% include 'section.jinja2' %}
        {% endfor %}
    </div>
{% endif %}

{% endblock %}